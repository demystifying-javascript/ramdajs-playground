{
  "name": "ramdajs-playground",
  "displayName": "Ramda Js Playground",
  "description": "This extension will enable you to access the Ramda Js's playground, right in your VS Code Editor.",
  "version": "1.1.0",
  "publisher": "demystifying-javascript",
	"license": "MIT",
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#f0efe7",
		"theme": "light"
	},
  "engines": {
    "vscode": "^1.76.0"
  },
  "keywords": [
    "functional javascript",
    "javascript",
    "functional programming",
    "functional js",
    "ramda",
    "ramda js",
    "playground",
    "cheatsheet",
    "ramda js documentation",
    "ramda js repl",
    "repl",
    "ramda javascript"
  ],
  "categories": [
    "Other"
  ],
  "repository": {
		"url": "hhttps://github.com/demystifying-javascript/ramdajs-playground"
	},
	"homepage": "hhttps://github.com/demystifying-javascript/ramdajs-playground",
	"bugs": {
		"url": "hhttps://github.com/demystifying-javascript/ramdajs-playground/issues"
	},
  "activationEvents": [
    "onCommand:ramdajs-playground.openPlayground",
    "onCommand:ramdajs-playground.openDocumentation"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ramdajs-playground.openPlayground",
        "title": "Open Ramda Js Playground"
      },
      {
        "command": "ramdajs-playground.openDocumentation",
        "title": "Open Ramda Js Documentation"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
